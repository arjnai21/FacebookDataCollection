{
  "manifest_version": 2,

  "name": "Facebook Data Collection",
  "description": "Collect Data from Facebook",
  "version": "1.0",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["https://www.facebook.com/*"],
      "js": ["main.js"]


    }

  ],

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]

}